[PROCEDURE]
NAME srcReplaceString
PROTOTYPE '(STRING s, STRING sAttribute, *ANY sNewValue),STRING'
PARAMETERS '(s, sAttribute, sNewValue)'
[COMMON]
DESCRIPTION 'Replace a string'
FROM ABC Source
MODIFIED '2003/07/20' '13:50:34'
