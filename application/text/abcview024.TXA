[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
sTemp       CSTRING(1024)
sWork       CSTRING(1024)
lpAttribute LONG
[END]
EMBED %ProcessedCode
[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
  sTemp = CLIP(s)
  lpAttribute = INSTRING(UPPER(sAttribute),UPPER(sTemp),1)
  IF lpAttribute
     LOOP WHILE lpAttribute
        sWork = SUB(sTemp,1,lpAttribute-1)
        sWork = sWork & sNewValue
        sTemp = SUB(sTemp,lpAttribute+LEN(sAttribute),LEN(sTemp)-(lpAttribute+LEN(sAttribute)-1))
        sWork = sWork & sTemp
        sTemp = sWork
        lpAttribute = INSTRING(UPPER(sAttribute),UPPER(sTemp),1)
     END
  ELSE
     sWork = sTemp
  END
  RETURN(sWork)
[END]
[END]
