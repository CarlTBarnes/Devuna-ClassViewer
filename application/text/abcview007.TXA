        loc:bTplEqu = TRUE
     OF 'WINEQU.CLW'
        loc:bWinEqu = TRUE
     OF 'WINDOWS.INC'
        loc:bWindows = TRUE
     END
  ELSE
     DELETE(ExtraModuleQ)
  END
END

