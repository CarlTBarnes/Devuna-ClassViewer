[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 5000
PROPERTY:END
OptionQ.Icon = 1
PUT(OptionQ)
MouseDownRow = ?TreeList{PROPLIST:MouseDownRow}
